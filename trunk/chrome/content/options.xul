			 preference="QuickFolders-Preferences-CurrentFolder-Background"
			 instantApply="true"
			 flex="1"/>

	<button label="&qf.button.currentFolderBackground.set;" 
	        class="narrow"
			onclick="QuickFolders.Options.setCurrentToolbarBackgroundCustom();"/>
	<menulist id="QuickFolders-CurrentFolder-Background-Select"
			  sizetopopup="none"
			  oncommand="QuickFolders.Interface.updateMainWindow();"
			  >
		<menupopup>
			<!-- values in this list are QuickFolders.Themes.themes.id values -->
			<menuitem label="&qf.currentFolderBackground.default;"
					  oncommand="QuickFolders.Options.setCurrentToolbarBackground('default', true);"
					  value="default"/>
			<menuitem label="&qf.currentFolderBackground.dark;"
					  oncommand="QuickFolders.Options.setCurrentToolbarBackground('dark', true);"
					  value="default"/>
			<menuitem label="&qf.currentFolderBackground.translucent;"
					  oncommand="QuickFolders.Options.setCurrentToolbarBackground('translucent', true);"
					  value="default"/>
			<menuitem label="&qf.currentFolderBackground.custom;"
					  oncommand="QuickFolders.Options.setCurrentToolbarBackground('custom', true);"
					  value="default"/>
		</menupopup>
