			          oncontextmenu="QuickFolders.Options.showAboutConfig(this,'quickfolders.recentfolders',true);return false;"
			          oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			          flex="1"
			/>
			<checkbox id="chkQuickMove"
			          label="&qf.checkbox.showQuickMove;"
			          preference="QuickFolders-Preferences-ShowQuickMove"
			          instantApply="true"
