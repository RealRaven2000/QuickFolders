			instantApply="true"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
		/>
		<checkbox label="&qf.checkbox.showCountInSubFolders;"
			preference="QuickFolders-Preferences-ShowCountInSubFolders"
			instantApply="true"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			tooltiptext="&qf.checkbox.showCountInSubFolders.tooltip;"
		/>
		<button label="&qf.button.configureTooltips;" 
		        oncommand="QuickFolders.Options.configureTooltips(this);return true;"
	     />		

	</vbox>
</groupbox>

<hbox>

	<groupbox flex="2">
		<caption label="&qf.label.behavior;"/>

		<checkbox
