			instantApply="true"
		/>

		<checkbox
			label="&qf.checkbox.showNonEmptyItalics;"
			preference="QuickFolders-Preferences-ShowFoldersWithMessagesItalic"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			instantApply="true"
		/>
		
		<checkbox
			label="&qf.checkbox.showNewMailItalics;"
			preference="QuickFolders-Preferences-ShowFoldersWithNewMailItalic"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			instantApply="true"
		/>
		

		<checkbox
			label="&qf.checkbox.highlightNewMail;"
			preference="QuickFolders-Preferences-ShowNewMail"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			instantApply="true"
		/>


	</vbox>
	<vbox>
		<checkbox label="&qf.checkbox.showUnreadMessageCount;"
			preference="QuickFolders-Preferences-ShowUnread"
			instantApply="true"
			oncommand="QuickFolders.Options.toggleBoolPreference(this);"
		/>

		<checkbox label="&qf.checkbox.showTotalMessageCount;"
