			          oncontextmenu="QuickFolders.Options.showAboutConfig(this,'quickfolders.quickMove',true);return false;"
			          oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			          flex="1"
			/>
			<checkbox id="chkToolbarMenu"
			          label="&qf.checkbox.tabMenuIcon;"
			          preference="QuickFolders-Preferences-ShowToolbarMenuIcon"
			          instantApply="true"
			          tooltiptext="&qf.checkbox.tabMenuIcon.tooltip;"
			          oncommand="QuickFolders.Options.toggleBoolPreference(this);"
			          flex="1"
			/>
			<hbox>
				<label value="&qf.toolbarMinimumHeight;"
					   tooltiptext="&qf.toolbarMinimumHeight.tooltip;"
					   href="https://www.mozdev.org/bugs/show_bug.cgi?id=25021"
					   onclick="QuickFolders.Util.openURL(event,'https://www.mozdev.org/bugs/show_bug.cgi?id=25021')"	
					   style = "text-decoration:underline;cursor:pointer;"
					   />
				<textbox id="toolbarMinHeight"
						 preference="QuickFolders-Preferences-ToolbarMinimalHeight"
						 instantApply="true"
						 style="max-width:5em; width:auto;"/>
			</hbox>	   
			<spacer flex="5" />
	</groupbox>
</hbox>

			</tabpanel>
			
			<tabpanel orient="vertical" id="QuickFolders-Options-advanced"
				label="&qf.tab.general;" flex="10" style="position:relative;">
				
